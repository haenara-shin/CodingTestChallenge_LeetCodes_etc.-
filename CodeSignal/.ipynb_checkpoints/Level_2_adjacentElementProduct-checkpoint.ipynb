    "def adjacentElementsProduct(inputArray):\n",
    "    temp = float('-inf')\n",
    "\n",
    "    for i in range(len(inputArray)-1):\n",
    "        if temp < inputArray[i] * inputArray[i+1]:\n",
    "            temp = inputArray[i] * inputArray[i+1]\n",
    "\n",
    "    return temp"
   ]
  }
