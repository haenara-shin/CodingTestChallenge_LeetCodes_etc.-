    "# 1906: 피보나치(큰 수) - 메모이제이션\n",
    "\n",
    "n = int(input())\n",
    "\n",
    "memoization = {0:0, 1:1}\n",
    "def fibonacci(n):\n",
    "    if n in memoization:\n",
    "        return memoization[n]\n",
    "    else:\n",
    "        memoization[n] = fibonacci(n-1) + fibonacci(n-2)\n",
    "        return memoization[n]\n",
    "print(fibonacci(n)%10009)"
   ]
  }
