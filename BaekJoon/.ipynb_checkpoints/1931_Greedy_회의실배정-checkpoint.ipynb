   "source": [
    "n = int(input())\n",
    "s = []\n",
    "for i in range(n):\n",
    "    first, second = map(int, input().split())\n",
    "    s.append([first, second])\n",
    "s = sorted(s, key=lambda a: (a[1], a[0]))\n",
    "last = 0\n",
    "cnt = 0\n",
    "for i, j in s:\n",
    "    if i >= last:\n",
    "        cnt += 1\n",
    "        last = j\n",
    "print(cnt)"
   ]
  }
